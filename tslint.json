{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "always"],
        "no-var-requires": false,
        "arrow-parens": true,
        "max-line-length": false,
        "no-console": false,
        "no-floating-promises": true,
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ]
    },
    "rulesDirectory": []
}